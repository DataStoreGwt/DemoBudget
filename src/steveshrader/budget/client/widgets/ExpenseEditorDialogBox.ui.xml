<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder' 
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:dt='urn:import:steveshrader.budget.client.widgets'>
  <ui:style>
  .buttonSave {
    float: left;
    margin-top: 10px;
    margin-left: 10px;
    margin-bottom: 10px;
  }
 
  .buttonCancel {
    float: left;
    margin-top: 10px;
    margin-bottom: 10px;
  }
      
  .buttonDelete {
    float: right;
    margin-top: 10px;
    margin-right: 10px;
    margin-bottom: 10px;
  }
  </ui:style>
  
  <g:DialogBox glassEnabled="true" animationEnabled="true" modal="true">
    <g:caption>New Expense</g:caption>
     <g:HTMLPanel ui:field="contents"> 
      <div>
        <g:Label text="Firstname"></g:Label><g:TextBox ui:field="firstname"></g:TextBox>
        <g:Label text="Lastname"></g:Label><g:TextBox ui:field="lastname"></g:TextBox>
        <g:Button ui:field="save" styleName="{style.buttonSave}">Save</g:Button>
        <g:Button ui:field="cancel" styleName="{style.buttonCancel}">Cancel</g:Button>
        <g:Button ui:field="delete" styleName="{style.buttonDelete}">Delete</g:Button>
      </div>
    </g:HTMLPanel>
  </g:DialogBox>
</ui:UiBinder>